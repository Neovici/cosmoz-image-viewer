		"mocha": "^5.0.0",
		"polymer-cli": "^1.9.11",
		"resize-observer-polyfill": "^1.5.1",
		"semantic-release": "^15.13.24",
		"sinon": "^7.5.0",
		"wct-istanbul": "^0.14.3",
		"wct-mocha": "^1.0.1",
		"web-animations-js": "^2.3.1"
	}
